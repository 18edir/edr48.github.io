{
    "name": "Chrome Extension Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com https://*.firebaseio-demo.com; object-src 'self'",

    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}
